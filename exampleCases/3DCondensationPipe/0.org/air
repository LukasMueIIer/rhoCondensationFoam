FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    pipe
    {
        type            zeroGradient;
    }
    FarField
    {
        type inletOutlet;
        inletValue uniform 1;
        value uniform 1;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.97;
    }
}
// ************************************************************************* //
